{
  "version": 2,
  "name": "Damian's CV",
  "alias": ["dafri.io", "www.dafri.io"],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [{ "src": "screenshot.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" } }, { "src": "next.config.js", "use": "@now/next" }],
  "routes": [
    { "src": "/pdf", "dest": "/screenshot.js" },
    { "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+", "headers": { "cache-control": "immutable" } }
  ]
}
