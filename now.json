{
  "version": 2,
  "name": "Damian's CV",
  "alias": ["dafri.io", "www.dafri.io"],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [{ "src": "pdf/index.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" } }, { "src": "package.json", "use": "@now/next" }],
  "routes": [
    {
      "src": "/pdf",
      "dest": "/pdf/index.js",
      "headers": {
        "cache-control": "max-age=3600"
      }
    },
    {
      "src": "/_next/static/(.*)",
      "headers": { "cache-control": "max-age=31557600" },
      "dest": "/_next/static/$1"
    }
  ]
}
