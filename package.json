{
  "name": "cv-builder",
  "version": "1.0.0",
  "description": "A Next.js based project to generate my personal CV.",
  "scripts": {
    "dev": "next",
    "dev:debug": "yarn dev --inspect=localhost:9227",
    "build": "next build",
    "export": "rm -rf docs && npm run build && next export -o docs && node ghPages",
    "start": "next start",
    "generatePdf": "ts-node --project tsconfig.pdf.json toPdf.ts http://localhost:3000 .next/cv.pdf"
  },
  "author": "Damian Frizzi",
  "license": "ISC",
  "dependencies": {
    "classnames": "2.2.6",
    "next": "8.0.3",
    "react": "16.8.3",
    "react-dom": "16.8.3"
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/next": "8.0.1",
    "@types/puppeteer": "1.12.1",
    "@types/react": "16.8.6",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-typescript": "1.1.1",
    "cssnano": "4.1.10",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "next-compose-plugins": "2.1.1",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.6.0",
    "prettier": "1.16.4",
    "puppeteer": "1.12.2",
    "replace-in-file": "3.4.3",
    "stylelint-config-prettier": "5.0.0",
    "ts-node": "8.0.2",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3333"
  }
}
